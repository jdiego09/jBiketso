<AnchorPane id="AnchorPane" prefHeight="570.0" prefWidth="700.0" styleClass="form" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jbiketso.controller.BitacoraAtencionController">
    <stylesheets>
        <URL value="@biketso.css" />
    </stylesheets>
   <children>
      <Pane prefHeight="55.0" prefWidth="744.0" styleClass="actionbar">
         <children>
            <JFXButton fx:id="jbtnSalir" alignment="CENTER" contentDisplay="CENTER" ellipsisString="" layoutX="704.0" layoutY="9.0" onAction="#regresar" prefHeight="25.0" prefWidth="26.0" styleClass="actionbuttonsalir" textOverrun="WORD_ELLIPSIS">
               <tooltip>
                  <Tooltip text="Regresar" />
               </tooltip>
            </JFXButton>
            <Button fx:id="btnGuardar" layoutX="14.0" layoutY="7.0" mnemonicParsing="false" onAction="#guardar" prefHeight="30.0" prefWidth="30.0" styleClass="actionbuttonguardar" text="Button">
               <tooltip>
                  <Tooltip text="Guardar" />
               </tooltip>
            </Button>
            <Button fx:id="btnLimpiar" layoutX="58.0" layoutY="7.0" mnemonicParsing="false" onAction="#limpiar" prefHeight="30.0" prefWidth="30.0" styleClass="actionbuttonlimpiar" text="Button">
               <tooltip>
                  <Tooltip text="Limpiar" />
               </tooltip>
            </Button>
            <Label fx:id="lblTitulo" alignment="CENTER" layoutX="254.0" layoutY="11.0" text="Bitácora de Atención" textFill="WHITE">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <JFXComboBox fx:id="jcmbTipoAtencion" labelFloat="true" layoutX="24.0" layoutY="82.0" prefHeight="39.0" prefWidth="212.0" promptText="Tipo" styleClass="edittext" visibleRowCount="2">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </JFXComboBox>
      <JFXDatePicker fx:id="jdtpFecha" editable="false" layoutX="517.0" layoutY="84.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" overLay="true" prefHeight="35.0" prefWidth="202.0" promptText="Fecha" styleClass="edittext" />
      <JFXTextField fx:id="jtxfCedula" labelFloat="true" layoutX="24.0" layoutY="152.0" onKeyPressed="#cedulaOnEnterKey" prefHeight="35.0" prefWidth="175.0" promptText="Cédula" styleClass="edittext" />
      <JFXButton fx:id="btnBusqueda" layoutX="199.0" layoutY="152.0" onAction="#busqueda" prefHeight="30.0" prefWidth="30.0" styleClass="buttonbuscar" />
      <JFXTextField fx:id="jtxfNombre" editable="false" focusTraversable="false" labelFloat="true" layoutX="245.0" layoutY="154.0" prefHeight="35.0" prefWidth="474.0" promptText="Nombre" styleClass="edittext" unFocusColor="#00796b" />
      <TableView fx:id="tbvBitacora" layoutX="24.0" layoutY="333.0" prefHeight="203.0" prefWidth="695.0">
        <columns>
          <TableColumn fx:id="tbcFecha" prefWidth="127.0" text="Fecha" />
          <TableColumn fx:id="tbcDetalle" prefWidth="561.0" text="Servicio brindado" />
        </columns>
      </TableView>
      <Separator layoutX="25.0" layoutY="323.0" prefHeight="3.0" prefWidth="695.0" />
      <Label layoutX="25.0" layoutY="296.0" text="Detalle atención brindada">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <JFXTextArea fx:id="jtxfDetalle" labelFloat="true" layoutX="25.0" layoutY="217.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onKeyPressed="#detalleOnKeyPress" prefHeight="65.0" prefWidth="656.0" promptText="Detalle" styleClass="edittext" />
      <Button fx:id="btnAgregar" layoutX="689.0" layoutY="255.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#agregar" onDragDetected="#agregar" prefHeight="30.0" prefWidth="30.0" styleClass="actionbuttonagregar" text="Agregar atención">
         <tooltip>
            <Tooltip text="Agregar dirección" />
         </tooltip>
      </Button>
   </children>
   <effect>
      <DropShadow />
   </effect>
</AnchorPane>
